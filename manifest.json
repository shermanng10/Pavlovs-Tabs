{
  "manifest_version": 2,

  "name": "TabKiller",
  "description": "This extension lets you know that you have too many tabs open, then closes them.",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    }
  },
  "background": {
    "scripts": ["output/background.js"],
    "persistent": false
  },
  "author": "Sherman Ng",
  "permissions": [
     "tabs", "http://*/*", "https://*/*"
  ]
}

